<%= form_for @user do |f| %>
  <%= f.label "First name" %>
  <%= f.text_field :first_name %>
  <%= f.label "Last name" %>
  <%= f.text_field :last_name %>
  <%= f.label "User name" %>
  <%= f.text_field :display_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Submit" %>
<% end %>
